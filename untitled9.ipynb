#Para intervalos de valores
import numpy
ls=numpy.linspace(0,10*numpy.pi,100)
ar=numpy.arange(0,10*numpy.pi,0.1)
print(ls)
print(ar)

import matplotlib.pyplot as plt
res=numpy.sin(ls)
plt.plot(ls,res)
plt.show()
